
s =("""   hello World
This is at multi line 
Print statement, no more
single line printing   """)

print (s)


print(s.strip() )
print (s.lstrip())
print (s.rstrip())

print (s.find("World"))
print (s.count("a"))
print (s.replace("Hello","Goodbye"))
print (s.islower())
print (s.title())

print (int(10))
print (bool(true))